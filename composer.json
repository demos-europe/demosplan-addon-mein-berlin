{
  "name": "demos-europe/demosplan-addon-mein-berlin",
  "description": "demosplan Addon to integrate with mein.berlin.de",
  "version": "v0.13",
  "type": "demosplan-addon",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "DemosEurope\\DemosplanAddon\\DemosMeinBerlin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DemosEurope\\DemosplanAddon\\DemosMeinBerlin\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "symfony/translation": "6.*.*",
    "demos-europe/demosplan-addon-installer": "^0",
    "demos-europe/demosplan-addon": "~v0.54",
    "doctrine/doctrine-bundle": "^2",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "gedmo/doctrine-extensions": "^3",
    "symfony/http-client": "6.*.*",
    "league/flysystem-aws-s3-v3": "^3.28",
    "league/flysystem-bundle": "^3.3",
    "league/flysystem-memory": "^3.28",
    "laminas/laminas-feed": "^2.23"
  },
  "config": {
    "allow-plugins": {
      "demos-europe/demosplan-addon-installer": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "ergebnis/phpstan-rules": "^2.0",
    "liip/functional-test-bundle": "^4",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.4.2",
    "phpstan/phpstan-webmozart-assert": "^1.2",
    "symfony/phpunit-bridge": "6.*.*",
    "symfony/test-pack": "^1.1",
    "thecodingmachine/phpstan-strict-rules": "^1.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse"
  }
}
